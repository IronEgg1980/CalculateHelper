<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yzw.ahaqth.calculatehelper">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.AssignActivity"></activity>
        <activity android:name=".views.SetupActivity" />
        <activity android:name=".views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.ShowRemain" />
        <activity android:name=".views.TestBrokenLineGraph" />
        <activity android:name=".views.Infomation" />
        <activity android:name=".views.ShowRecordDetailsActivity" />
        <activity android:name=".views.HistoryActivity" />
        <activity android:name=".views.AssignActivity2" />
        <activity android:name=".views.ItemManageActivity" />
        <activity android:name=".views.PersonManageActivity" />
        <activity
            android:name=".views.InputActivity"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>